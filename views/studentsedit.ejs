<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>

    <body>
        <h1>Halaman Students</h1>
        <a href="/">Back to Home</a>
        <table>
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
                <form class="" action="/students/edit/<%=data[0].id%>" method="POST">
                    <input type="text" name="first_name" placeholder="" value="<%=data[0].first_name%>">
                    <input type="text" name="last_name" placeholder="" value="<%=data[0].last_name%>">
                    <input type="text" name="email" placeholder="" value="<%=data[0].email%>">
                    <input type="submit" value="SUBMIT">
                </form>
            </tbody>
        </table>
    </body>
</html>